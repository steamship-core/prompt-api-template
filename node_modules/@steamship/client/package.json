{
  "name": "@steamship/client",
  "version": "2.2.40",
  "description": "Steamship is the fastest way to add Language AI to your software.",
  "main": "build/main/index.js",
  "module": "build/module/index.js",
  "type": "module",
  "typings": "build/module/index.d.ts",
  "repository": "https://github.com/steamship-core/typescript-client",
  "license": "MIT",
  "keywords": [],
  "exports": {
    "./*": {
      "require": "./build/main/index.js",
      "import": "./build/module/index.js"
    },
    ".": {
      "require": "./build/main/index.js",
      "import": "./build/module/index.js"
    },
    "./package.json": "./package.json",
    "./types": "./build/main/index.d.ts"
  },
  "contributors": [
    {
      "name": "Steamship Team",
      "email": "contact@steamship.com",
      "url": "https://www.steamship.com/"
    }
  ],
  "scripts": {
    "build": "run-p build:*",
    "_build:main_": "tsc -p tsconfig.json",
    "build:module_types": "tsc -p tsconfig.module.json",
    "build:module": "babel src --no-babelrc --out-dir build/module --extensions \".ts,.tsx\" --source-maps inline --config-file ./.babelrc.es",
    "build:main": "babel src --out-dir build/main --extensions \".ts,.tsx\" --source-maps inline",
    "fix": "run-s fix:*",
    "fix:prettier": "prettier \"src/**/*.ts\" --write",
    "fix:lint": "eslint src --ext .ts --fix",
    "test": "run-s build test:*",
    "_test:lint": "eslint src --ext .ts",
    "_test:prettier": "prettier \"src/**/*.ts\" --list-different",
    "_test:spelling": "cspell \"{README.md,.github/*.md,src/**/*.ts}\"",
    "test:unit": "ts-jest --runInBand",
    "check-cli": "run-s test diff-integration-tests check-integration-tests",
    "check-integration-tests": "run-s check-integration-test:*",
    "diff-integration-tests": "mkdir -p diff && rm -rf diff/test && cp -r test diff/test && rm -rf diff/test/test-*/.git && cd diff && git init --quiet && git add -A && git commit --quiet --no-verify --allow-empty -m 'WIP' && echo '\\n\\nCommitted most recent integration test output in the \"diff\" directory. Review the changes with \"cd diff && git diff HEAD\" or your preferred git diff viewer.'",
    "watch:build": "tsc -p tsconfig.json -w",
    "cov": "run-s build test:unit cov:html cov:lcov && open-cli coverage/index.html",
    "cov:html": "nyc report --reporter=html",
    "cov:lcov": "nyc report --reporter=lcov",
    "cov:send": "run-s cov:lcov && codecov",
    "cov:check": "nyc report && nyc check-coverage --lines 100 --functions 100 --branches 100",
    "doc": "run-s doc:html && open-cli build/docs/index.html",
    "doc:html": "typedoc src/ --exclude **/*.spec.ts --target ES6 --mode file --out build/docs",
    "doc:json": "typedoc src/ --exclude **/*.spec.ts --target ES6 --mode file --json build/docs/typedoc.json",
    "doc:publish": "gh-pages -m \"[ci skip] Updates\" -d build/docs",
    "version": "2.2.40",
    "reset-hard": "git clean -dfx && git reset --hard && yarn",
    "prepare-release": "run-s reset-hard test cov:check doc:html version doc:publish",
    "prepare": "husky install"
  },
  "engines": {
    "node": ">=10"
  },
  "dependencies": {
    "@babel/runtime": "^7.20.7",
    "axios": "^1.2.2",
    "form-data": "^4.0.0",
    "regenerator-runtime": "^0.13.11",
    "tslog": "^3.3.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.20.7",
    "@babel/core": "^7.20.7",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-transform-runtime": "^7.16.8",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/http-cache-semantics": "^4.0.1",
    "@types/jest": "^29.2.4",
    "@types/node": "^18.11.17",
    "@types/node-fetch": "^2.6.2",
    "@types/regenerator-runtime": "^0.13.1",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.1",
    "babel-plugin-add-module-exports": "^1.0.4",
    "codecov": "^3.5.0",
    "cspell": "^4.1.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-functional": "^3.0.2",
    "eslint-plugin-import": "^2.22.0",
    "husky": "^8.0.2",
    "jest": "^29.3.1",
    "jest-esm-transformer": "^1.0.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "open-cli": "^7.1.0",
    "prettier": "^2.7.1",
    "prettier-plugin-organize-imports": "^3.1.1",
    "standard-version": "^9.0.0",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "files": [
    "build/main",
    "build/module",
    "!**/*.spec.*",
    "!**/*.json",
    "CHANGELOG.md",
    "LICENSE",
    "README.md"
  ],
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "prettier": {
    "singleQuote": true
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "exclude": [
      "**/*.spec.js"
    ]
  }
}
